# Archivo de ejemplo de variables de entorno
# Copie este archivo como .env.development, .env.test o .env.production
# y complete con sus valores correspondientes

# Servidor
PORT=3000
API_VERSION=v1
NODE_ENV=development

# Base de datos
DB_HOST=localhost
DB_PORT=5432
DB_NAME=entregas_dev
DB_USER=your_db_user
DB_PASSWORD=your_db_password

# Tango Connect
TANGO_CONNECT_URL=https://api.tangoconnect.axoft.com
TANGO_CONNECT_CLIENT_ID=your_client_id
TANGO_CONNECT_CLIENT_SECRET=your_client_secret

# JWT
JWT_SECRET=your_secret_key
JWT_EXPIRES_IN=1d